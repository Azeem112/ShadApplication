    "sDom": '<"dt-panelmenu clearfix"Tfr>t<"dt-panelfooter clearfix"ip>',
    "oTableTools": {
    "sSwfPath": "vendor/plugins/datatables/extensions/TableTools/swf/copy_csv_xls_pdf.swf"
    }
    });

    $('#datatable4').dataTable({
    "aoColumnDefs": [{
    'bSortable': false,
    'aTargets': [-1]
    }],
    "oLanguage": {
    "oPaginate": {
    "sPrevious": "",
    "sNext": ""
    }
    },
    "iDisplayLength": 5,
    "aLengthMenu": [
