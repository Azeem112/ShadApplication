    <!-- Begin: Content -->
    <div class="panel">
        <div class="panel-body pn">

            <form class="form-horizontal" role="form" style="margin-left:10px">

                <div class=" row form-group" style="margin-left:25px">
                    <br>


                    <label class="col-md-12"> <h3>Account owner</h3></label>

                    <label class="col-md-4">Logged in As: <span style="font-size:16px">user@gmail.com</span></label>
                    <label class="col-md-2 col-md-offset-1"><a data-toggle="modal" data-target="#myModal1">Change Password</a></label>

                </div>



                <div class=" row form-group">


                    <!-- End: Topbar -->
                    <label class="col-md-6" style="margin-left: 35px"> <h3>Edit Details</h3></label>

                    <div style="width: 80%;">
                        <div class="tab-pane">
                            <br><br><br>
                            <div class="row">
                                <div class="row form-group required">
                                    <label class="col-md-2 col-xs-12 text-right control-label">Email:</label>
                                    <div class="col-md-8 col-xs-11 col-xs-offset-1 col-md-offset-0">
                                        <input type="email" class="form-control" placeholder="Enter Email here" required="required">
                                    </div>
                                </div>
                                <div class="row form-group required">
                                    <label class="col-md-2 col-xs-12 text-right control-label">Password:</label>
                                    <div class="col-md-8 col-xs-11 col-xs-offset-1 col-md-offset-0">
                                        <input type="password" class="form-control" placeholder="Provide password" required="required">
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <label class="col-md-2 col-xs-12 text-right">Full Name:</label>
                                    <div class="col-md-8 col-xs-11 col-xs-offset-1 col-md-offset-0">
                                        <input type="text" class="form-control" placeholder="Enter Full name here">
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <label class="col-md-2 col-xs-12 text-right">Phone:</label>
                                    <div class="col-md-8 col-xs-11 col-xs-offset-1 col-md-offset-0">
                                        <input type="text" class="form-control" placeholder="Enter Phone Number name here">
                                    </div>
                                </div>
                                <div class="row form-group ">
                                    <label class="col-md-2 col-xs-12 text-right control-label">Status:</label>
                                    <div class="col-md-8 col-xs-11 col-xs-offset-1 col-md-offset-0">
                                        <select class="form-control" onchange="calculate()" id="tax" style="margin-bottom: 3%;">
                                            <option selected disabled value="0.00">-Select Status-</option>
                                            <option value="">Active</option>
                                            <option value="">Inactive</option>
                                        </select>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <br>
                        <br>

                        <button type="button" class="btn btn-success col-md-2 col-md-offset-2 col-xs-offset-5" style=" border-radius 5px">Save</button>
                    </div>
                </div>
            </form><br><br><br><br><br>

