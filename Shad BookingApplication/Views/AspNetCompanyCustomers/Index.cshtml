@model IEnumerable<Shad_BookingApplication.Models.AspNetCompanyCustomer>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_SuperAdminNavigationBar.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Adress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PostCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeZone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BateofBirth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RefferedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contradiction)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AspNetCompanyNotifination.EmailBeforeArrive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AspNetCustomerDetail.BussinessName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Adress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeZone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BateofBirth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RefferedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contradiction)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetCompanyNotifination.EmailBeforeArrive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetCustomerDetail.BussinessName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
