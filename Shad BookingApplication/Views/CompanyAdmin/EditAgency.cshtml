jQuery(window).load(function () {
	var region_id = $("#region_id1").val();
	
	$.ajax({
		type: "GET",
		url: "/CompanyAdmin/Get_WorkingTime",
		data: { "region_id": region_id },
		contentType: "application/json; charset=utf-8",
		dataType: "json",
		success: function (response) {
			alert(response);
			
			var row_no = 1;
			var j = 1;
			for (var i = 0; i < response.length; i++) {
				if (i == 0) {
					row_no = 1;
				} else if (i == 1) {
					row_no = 2;
				} else if (i == 2) {
					row_no = 3;
				} else if (i == 3) {
					row_no = 4;
				} else if (i == 4) {
					row_no = 5;
				} else if (i == 5) {
					row_no = 6;
				} else if (i == 6) {
					row_no = 7;
				}

				console.log(row_no);
				if (response[i].isoff.toLowerCase() === "True".toLowerCase()) {
					var num = row_no;

					$('#c' + num).attr('checked', true);
					document.getElementById("i" + ((num - 1) * 4 + 1)).disabled = true;
					document.getElementById("i" + ((num - 1) * 4 + 2)).disabled = true;
					document.getElementById("i" + ((num - 1) * 4 + 3)).disabled = true;
					document.getElementById("i" + ((num - 1) * 4 + 4)).disabled = true;
					document.getElementById("r" + num).style.backgroundColor = "lightgrey";

				}
				else if (response[i].isoff.toLowerCase() === "False".toLowerCase()) {
					$("#i" + ((j - 1) * 4 + 1)).val(response[i].StartTime);
					$("#i" + ((j - 1) * 4 + 2)).val(response[i].EndTime);
					$("#i" + ((j - 1) * 4 + 3)).val(response[i].LunchFrom);
					$("#i" + ((j - 1) * 4 + 4)).val(response[i].LunchToo);
				}
				j++;
			}
		},
		failure: function (response) {
			alert(response.responseText);
		},
		error: function (response) {
			alert(response.responseText);
		}
	});
});
</script>
